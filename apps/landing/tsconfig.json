{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig/next.json",
  "compilerOptions": {
    "baseUrl": "."
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.mjs",
    ".next/types/**/*.ts",
    ".eslintrc.js",
    "postcss.config.js",
    "tailwind.config.js",
    "prettier.config.js"
  ],
  "exclude": ["node_modules"]
}
