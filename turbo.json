{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ]
        },
        "type-check": {
            "dependsOn": [
                "^build"
            ]
        },
        "test": {},
        "ci-check": {},
        "lint": {},
        "lint:fix": {
            "cache": false
        },
        "prettier": {},
        "prettier:fix": {
            "cache": false
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}