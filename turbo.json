{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "cron#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "docs#build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "test": {},
    "lint": {},
    "lint:fix": {},
    "prettier": {},
    "prettier:fix": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
