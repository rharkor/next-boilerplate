{
  "name": "@next-boilerplate/app",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run is-initialized && npx prisma migrate dev && cross-env FORCE_COLOR=1 next dev",
    "build": "next build",
    "start": "npm run deploy-db:prod && npm run seed && next start",
    "start:with-cron": "npm run deploy-db:prod && npm run seed && concurrently \"next start\" \"npm run cron\"",
    "deploy-db:prod": "npx prisma migrate deploy",
    "cron": "npx tsx ./crons/index.ts",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "postinstall": "npx patch-package && prisma generate",
    "seed": "NODE_ENV=development prisma db seed",
    "is-initialized": "cd ../scripts && npm run is-initialized"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.454.0",
